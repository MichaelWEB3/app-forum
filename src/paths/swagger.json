{
    "swagger": "2.0",
    "info": {
        "title": "Documentação da API",
        "description": "Documentação da API do Next.js com NextAuth.js",
        "version": "1.0.0"
    },
    "basePath": "/api",
    "schemes": [
        "http",
        "https"
    ],
    "paths": {
        "/auth/callback": {
            "post": {
                "summary": "Rota de retorno de chamada para autenticação",
                "responses": {
                    "200": {
                        "description": "Sucesso"
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                }
            }
        },
        "/auth/signin": {
            "post": {
                "summary": "Rota de login",
                "responses": {
                    "200": {
                        "description": "Sucesso"
                    },
                    "401": {
                        "description": "Não autorizado"
                    }
                }
            }
        },
        "/auth/signout": {
            "post": {
                "summary": "Rota de logout",
                "responses": {
                    "200": {
                        "description": "Sucesso"
                    }
                }
            }
        }
    }
}